<%- contentFor('body') %>
<div class="jumbotron text-center">
  <form action="/loginDocente" method="GET"> 
    <!-- <form action="/venta" method="GET">--> 
      <div class="row">
        <div class="col-sm-6 col-sm-offset-3">
          <h3>Ingrese su información de usuario</h3>
          <br>
          <div class="form-group">
            <label>Correo institucional</label>
            <input type="text" name="aPaterno" class="form-control" required placeholder="alguien@tectijuana.edu.mx">
          </div>
          <div class="form-group">
          <label>Constraseña</label>
            <input type="password" name="aMaterno" class="form-control" required placeholder="********">
          </div>

          <div class="form-group text-right">
            <a href="/solicitudes" class="btn btn-primary btn-lg">Iniciar sesión</a>
          </div> 
      </div>
     </div>
    </form>
</div>